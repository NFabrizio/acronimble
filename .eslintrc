{
  "plugins": ["react"],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 8,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "modules": true,
      "arrowFunctions": true,
      "classes": true
    }
  },
  "env": {
    "es6": true,
    "browser": true,
    "jest": true
  },
  "globals": {
    "module": false,
    "process": false
  },
  "extends": ["eslint:recommended", "plugin:react/recommended", "eslint-config-prettier"],
  "rules": {
    "react/prop-types": [0]
  }
}
